  <section id="contact" class="section-block">
  <h2>Get in touch</h2>
  <div class= "container">

  <div>All projects at Magpie Studios Kent are of the highest fidelity at an affordable rate. If you need anymore information or would like to talk about your project, get in contact via the form below. We look forward to working with you.</div>
  <div style="padding-top:1rem; " >



    <%= form_for Contact.new do |f| %>
    <div class="form-group">
      <%= f.label :name, "Name *" %><br />
      <%= f.text_field :name, autocomplete: "email", class: "form-control", :required => true  %>
    </div>
    <div class="form-group">
      <%= f.label :email, "Email address *" %><br />
      <%= f.email_field :email,  autocomplete: "email", class: "form-control", :required => true  %>
    </div>
    <div class="form-group">
      <%= f.label :phone_number, "Phone number *" %><br />
      <%= f.text_field :phone_number, autocomplete: "email", class: "form-control", :required => true  %>
    </div>

    <div class="form-group">
      <%= f.label :message %><br />
      <%= f.text_area :message, as: :text, class: "form-control", rows: 5  %>
    </div>

    <div class="hidden">
      <%= f.label :nickname %><br />
      <%= f.text_field :nickname, hint: 'leave this field blank' %>
    </div>

    <div class="actions">
      <%= f.submit "Send message", class: "btn btn-primary" %>
    </div>
    <% end %>

  </div>
</div>
  </section>




